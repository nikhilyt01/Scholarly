{
    "version": 2,
    "builds": [
      {
        "src": "src/index.ts",  
        "use": "@vercel/node", 
        "config": {
          "maxLambdaSize": "50mb"  
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)", 
        "dest": "src/index.ts" ,
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*"
        }
      }
    ]
   
  }